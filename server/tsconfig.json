{
  "compilerOptions": {
    // "module": "commonjs",
    "target": "ES6",
    "allowJs": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "module": "esnext",
    "strict": false,
    "importHelpers": true,
    "outDir": "../build/",
    // "esModuleInterop":true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports":true,
    "rootDir": "../",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "preserve",
    "baseUrl": ".",
  }
}